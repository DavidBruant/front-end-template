{
  "name": "front-end-template",
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "predev": "mkdir -p _site/build && ln -f build/* _site/build",
    "dev": "npm-run-all --parallel dev:* start",
    "dev:jekyll": "bundle exec jekyll build -w --incremental --config _config.yml,_config_dev.yml",
    "dev:rollup": "rollup -c -w",
    "start": "http-server _site"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^3.17.3",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-svelte": "^7.1.3",
    "sass": "^1.58.3",
    "svelte": "^3.55.1",
    "svelte-preprocess": "^4.7.0"
  }
}
